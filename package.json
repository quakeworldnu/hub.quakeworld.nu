{
  "name": "qw-server-browser",
  "version": "0.1.0",
  "description": "QuakeWorld Server Browser",
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": "vikpe/qw-server-browser",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:convex": "npm-run-all convex:init --parallel convex:server dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "biome check --write src",
    "lint": "biome lint src",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch --coverage",
    "convex:server": "convex dev",
    "convex:init": "convex dev --run init --until-success",
    "supabase:types": "supabase gen types typescript --project-id ncsphkjfominimxztjip > src/pages/games/services/supabase/database.types.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "supabase": "^1.176.10",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@faker-js/faker": "^8.4.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@reduxjs/toolkit": "^2.2.5",
    "@sentry/react": "^8.9.2",
    "@supabase/supabase-js": "2.43.5",
    "@uidotdev/usehooks": "^2.4.1",
    "autoprefixer": "^10.4.19",
    "classnames": "^2.5.1",
    "convex": "^1.12.1",
    "copy-text-to-clipboard": "^3.2.0",
    "dayjs": "^1.11.11",
    "firebase-tools": "^13.11.2",
    "formik": "^2.4.6",
    "lodash.groupby": "^4.6.0",
    "nanoid": "^5.0.7",
    "postcss": "^8.4.38",
    "query-string": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-idle-timer": "^5.7.2",
    "react-modern-drawer": "^1.3.1",
    "react-range": "^1.8.14",
    "react-redux": "^9.1.2",
    "react-swipeable": "^7.0.1",
    "react-toastify": "^9.1.3",
    "sass": "^1.77.6",
    "screenfull": "^6.0.2",
    "tailwindcss": "^3.4.4",
    "urlcat": "2.0.4",
    "use-double-tap": "^1.3.6",
    "use-long-press": "^3.2.0",
    "usehooks-ts": "^2.12.1",
    "vite": "^5.3.1"
  }
}
