{
  "name": "qw-server-browser",
  "version": "0.1.0",
  "description": "QuakeWorld Server Browser",
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": "vikpe/qw-server-browser",
  "scripts": {
    "dev": "vite",
    "dev:convex": "npm-run-all convex:init --parallel convex:server dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "yarn prettier --write src",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch --coverage",
    "convex:server": "convex dev",
    "convex:init": "convex dev --run init --until-success",
    "supabase:types": "supabase gen types typescript --project-id ncsphkjfominimxztjip > src/pages/recent_games/services/supabase/database.types.ts"
  },
  "devDependencies": {
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
