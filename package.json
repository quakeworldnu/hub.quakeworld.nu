{
  "name": "qw-server-browser",
  "version": "0.1.0",
  "description": "QuakeWorld Server Browser",
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": "vikpe/qw-server-browser",
  "scripts": {
    "dev": "vite",
    "dev:convex": "npm-run-all convex:init --parallel convex:server dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "yarn prettier --write src",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch --coverage",
    "convex:server": "convex dev",
    "convex:init": "convex dev --run init --until-success"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.6",
    "ag-grid-community": "^30.2.0",
    "ag-grid-react": "^30.2.0",
    "autoprefixer": "^10.4.16",
    "browser-monads": "^1.0.0",
    "classnames": "^2.3.2",
    "convex": "^1.3.1",
    "copy-text-to-clipboard": "^3.2.0",
    "firebase-tools": "^12.6.0",
    "formik": "^2.4.5",
    "lodash.groupby": "^4.6.0",
    "postcss": "^8.4.30",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modern-drawer": "^1.2.2",
    "react-redux": "^8.1.2",
    "sass": "^1.68.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9"
  }
}
